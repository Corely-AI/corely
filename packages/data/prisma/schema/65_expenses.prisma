model Expense {
  id              String   @id @default(cuid())
  tenantId        String
  merchant        String
  totalCents      Int
  currency        String
  category        String?
  issuedAt        DateTime
  custom          Json?
  createdByUserId String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@index([tenantId])
  @@index([tenantId, issuedAt])
}
