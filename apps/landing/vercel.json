{
  "rewrites": [
    { "source": "/vi/nails", "destination": "https://nails.corely.one" },
    { "source": "/vi/nails/(.*)", "destination": "https://nails.corely.one/$1" },
    { "source": "/favicon.svg", "destination": "https://nails.corely.one/favicon.svg" },
    { "source": "/assets/(.*)", "destination": "https://nails.corely.one/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
